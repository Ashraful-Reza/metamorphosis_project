<odoo>
    <record id="view_move_form_mushak" model="ir.ui.view">
        <field name="name">account.move.mushak.report.button</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='button_box']" position="replace">
                <button name="action_print_mushak"
                    type="object"
                    class="oe_stat_button"
                    icon="fa-print"
                    invisible="1">
                    <field name="name">Mushak Print</field>
                </button>
            </xpath>
        </field>
    </record>
</odoo>